{
    "configurations": [
        {
            "name": "Linux",
            "intelliSenseMode": "gcc-x64",
            "includePath": [
                "${workspaceFolder}/inc",
                "${workspaceFolder}/src",
                "${workspaceFolder}/lib",
                "${workspaceFolder}/inc/sqlitemoderncpp",
                "${workspaceFolder}/inc/simpleini",
                "${workspaceFolder}/inc/magicenum",
                "/usr/local/include/**",
                "/usr/include/**",
                "${workspaceFolder}/inc"
            ],
            "defines": [
                "_DEBUG"
            ],
            "cStandard": "c11",
            "cppStandard": "c++23",
            "compileCommands": "${workspaceFolder}/compile_commands.json",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}